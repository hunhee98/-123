	<information>
		<name>#123</name>
		<version></version>
		<description>
			<![CDATA[ 2025.07.28 ]]>
		</description>
		<license>
			<![CDATA[ MIT License ]]>
		</license>
	</information>

	<author>
		<name>Tistory and hunhee98</name>
		<homepage>http://www.tistory.com</homepage>
		<email>tistoryblog@daum.net</email>
	</author>

	<contest>
		<description>2015 반응형 스킨 공모전 기념스킨</description>
		<preview>//t1.daumcdn.net/tistory_admin/static/admin/new/skin_preview_ray.png</preview>
		<document>http://notice.tistory.com/2271</document>
	</contest>

	<default>
		<recentEntries>4</recentEntries>
		<recentComments>4</recentComments>
		<recentTrackbacks>4</recentTrackbacks>
		<itemsOnGuestbook>10</itemsOnGuestbook>
		<tagsInCloud>30</tagsInCloud>
		<sortInCloud>3</sortInCloud>
		<expandComment>1</expandComment>
		<lengthOfRecentNotice>25</lengthOfRecentNotice>
		<lengthOfRecentEntry>27</lengthOfRecentEntry>
		<lengthOfRecentComment>30</lengthOfRecentComment>
		<lengthOfRecentTrackback>30</lengthOfRecentTrackback>
		<lengthOfLink>30</lengthOfLink>
		<showListOnCategory>1</showListOnCategory>
		<showListOnArchive>1</showListOnArchive>
		<showListLock>1</showListLock>

		<commentMessage>
			<none>0</none>
			<single>1</single>
		</commentMessage>

		<trackbackMessage>
			<none>0</none>
			<single>1</single>
		</trackbackMessage>

		<tree>
			<color>000000</color>
			<bgColor>ffffff</bgColor>
			<activeColor>000000</activeColor>
			<activeBgColor>eeeeee</activeBgColor>
			<labelLength>27</labelLength>
			<showValue>1</showValue>
		</tree>

		<contentWidth>820</contentWidth>
	</default>

	<!-- ▼ 사용자 입력용 스킨 변수 -->
	<variables>
		<variable>
			<name>adsense_client</name>
			<label>AdSense Client ID</label>
			<type>text</type>
			<default>ca-pub-0000000000000000</default>
		</variable>
		<variable>
			<name>adsense_slot</name>
			<label>AdSense Slot ID</label>
			<type>text</type>
			<default></default>
		</variable>
	</variables>
</skin>
